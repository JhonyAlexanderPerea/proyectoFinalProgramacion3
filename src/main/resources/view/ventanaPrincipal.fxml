<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #4d82bc;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ventanaPrincipalController">
   <children>
      <Label layoutX="211.0" layoutY="14.0" prefHeight="49.0" prefWidth="378.0" text="VENTANA PRINCIPAL">
         <font>
            <Font name="Work Sans SemiBold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnIniciarSesion" layoutX="358.0" layoutY="508.0" mnemonicParsing="false" onAction="#abrirViewInicioSesion" text="Iniciar sesion" />
      <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="14.0" layoutY="556.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/iconoAtras.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Button fx:id="btnSalir" layoutX="14.0" layoutY="556.0" mnemonicParsing="false" onAction="#finalizarEjecucion" opacity="0.0" prefHeight="30.0" prefWidth="30.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
